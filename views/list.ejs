
    <%- include("header"); -%>
    <div class="box">
      <h1 id="heading"><%= listTitle %></h1>
    </div>

    <div class="box">
      <% for (var i=0; i < newListItems.length; i++){ %>
      <div class="item">
        <input type="checkbox" name="checkbox" />
        <p><%= newListItems[i] %></p>
      </div>
      <% }; %>
      <form action="/" method="post">
        <input
          type="text"
          name="newtodo"
          placeholder="new todo"
          autocomplete="off"
        />
        <button type="submit" name="list" value="<%= listTitle %>">+</button>
      </form>
    </div>
 <%- include("footer"); -%>